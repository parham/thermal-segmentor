{
    "model" : {
        "name" : "unet_resnet18",
        "config" : {
            "channels": 3,
            "num_classes": 3,
            "weights": "imagenet"
        }
    },
    "metrics" : {
        "smp": {
            "mode" : "multilabel",
            "threshold" : 0.5,
            "num_classes" : 3,
            "metrics": {
              "f1" : {
               "reduction" : "macro",
               "class_weights" : null,
               "zero_division" : 1
               },
              "iou" : {
                "reduction" : "macro",
               "class_weights" : null,
               "zero_division" : 1
              }
            }
        }
    },
    "loss": {
        "name": "soft_bce",
        "config": {
          "reduction": "mean",
          "number_classes" : 3
        }
    },
    "optimizer": {
        "name": "SGD",
        "config": {
          "lr": 0.5,
          "momentum": 0.9,
          "weight_decay": 0.005
        }
    },
    "scheduler" : {
        "name" : "CosineAnnealingLR",
        "config" : {
          "start_value": 0.5,
          "end_value": 0,
          "cycle_size": 8,
          "cycle_mult": 2,
          "mult": 0.5
        }
    },
    "pipeline" : {
        "unet50_train" : {
            "wrapper" : "platesim_wrapper",
            "max_epoch" : 10,
            "batch_size" : 1
        }
    }
}
